{
  "name": "autowow",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:email": "NODE_OPTIONS='--loader ts-node/esm' ts-node --project tsconfig.json scripts/test-email.ts",
    "send:customer-emails": "NODE_OPTIONS='--loader ts-node/esm' ts-node --project tsconfig.json scripts/send-customer-video.ts",
    "db:start": "cd localDB && docker-compose up -d",
    "db:stop": "cd localDB && docker-compose down -v",
    "db:restart": "cd localDB && docker-compose restart",
    "db:reset": "cd localDB && docker-compose down -v && docker-compose up -d",
    "db:logs": "cd localDB && docker-compose logs -f",
    "db:status": "cd localDB && docker-compose ps"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.699.0",
